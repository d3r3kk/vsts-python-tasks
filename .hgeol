[patterns]

**.ico  = BIN
**.bmp  = BIN
**.png  = BIN
**.jpg  = BIN
**.jpeg = BIN

# Some Windows-specific files should always be CRLF

**.bat  = CRLF
**.cmd  = CRLF

# All other files (which presumably are human-editable) are "native".
# This must be the last rule!

** = native

[repository]
native = LF
